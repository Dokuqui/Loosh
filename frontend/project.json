{
  "name": "frontend",
  "root": "frontend",
  "sourceRoot": "frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd frontend && ng build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd frontend && ng serve"
      }
    }
  }
}
